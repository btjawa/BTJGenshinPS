{
  "name": "btjgenshinps",
  "version": "1.0.0",
  "description": "A Click-To-Run Application To Help You Access Grasscutter GenshinPS.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ BTJGenshinPS --platform=win32  --overwrite  --icon=./dist/favicon_64.ico --arch=ia32 && asar pack ./BTJGenshinPS-win32-ia32/resources/app ./BTJGenshinPS-win32-ia32/resources/app.asar && rm -rf ./BTJGenshinPS-win32-ia32/resources/app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btjawa/BTJGenshinPS.git"
  },
  "keywords": [
    "Click-To-Run",
    "Grasscutter",
    "GenshinPS"
  ],
  "author": "BTJ",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/btjawa/BTJGenshinPS/issues"
  },
  "homepage": "https://github.com/btjawa/BTJGenshinPS#readme",
  "devDependencies": {
    "electron": "^26.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "electron-log": "^4.4.8",
    "electron-packager": "^17.1.2",
    "izitoast": "^1.4.0"
  }
}
